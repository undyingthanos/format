  "main": "src/bin.mjs",
  "type": "module",
  "bin": {
    "f": "src/bin.mjs",
    "format": "src/bin.mjs"
  },
  "scripts": {
    "start": "t",
    "build": "NODE_ENV=production magic build",
    "prod": "NODE_ENV=production magic build serve",
    "clean": "magic clean",
    "serve": "magic serve",
    "dev": "magic dev",
    "test": "t",
    "format": "node src/bin.mjs f -w --exclude docs",
